{
  "name": "todo-list",
  "version": "0.0.1",
  "description": "A study of composition api and testing",
  "productName": "Todo List",
  "author": "",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
